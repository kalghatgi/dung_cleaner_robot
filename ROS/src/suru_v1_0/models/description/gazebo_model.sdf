      <sensor name="uvc_robot_v3_lidar" type="ray">
        <always_on>true</always_on>
        <visualize>true</visualize>
        <pose>0 0 0 0 0 0</pose>
        <update_rate>25</update_rate>
        <ray>
          <scan>
            <horizontal>
              <samples>600</samples>
              <resolution>0.500000</resolution>
              <min_angle>-2.6179917</min_angle>
              <max_angle>2.6179917</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.100000</min>
            <max>20.000000</max>
            <resolution>0.010000</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.01</stddev>
          </noise>
        </ray>
        <plugin name="uvc_robot_v3_laserscan" filename="libgazebo_ros_ray_sensor.so">
          <ros>
            <!-- <namespace>/tb3</namespace> -->
            <remapping>~/out:=scan</remapping>
          </ros>
          <output_type>sensor_msgs/LaserScan</output_type>
          <frame_name>lidar_frame</frame_name>
        </plugin>
      </sensor>
