  <!-- ********************* All Joints ********************* -->
  <joint name="base_joint" type="fixed">
    <pose relative_to='base_footprint'>0 0 0.075 0 0 0</pose>
    <parent>base_footprint</parent>
    <child>base_link</child>
  </joint>

    <joint name='joint_left' type='revolute'>
      <pose relative_to='base_link'>0 0.35 0.0249 0 0 0</pose>
      <parent>base_link</parent>
      <child>link_left</child>
      <axis>
        <xyz>0 -1 0</xyz>
        <limit>
          <effort>6</effort>
          <velocity>1.2</velocity>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
      </axis>
    </joint>

    <joint name='joint_right' type='revolute'>
      <pose relative_to='base_link'>0 -0.35 0.0249 0 0 0</pose>
      <parent>base_link</parent>
      <child>link_right</child>
      <axis>
        <xyz>0 -1 0</xyz>
        <limit>
          <effort>6</effort>
          <velocity>1.2</velocity>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
      </axis>
    </joint>

    <joint name='joint_castor_fl' type='fixed'>
      <pose relative_to='base_link'>-0.37307 -0.22458 0 0 0 3.1416</pose>
      <parent>base_link</parent>
